{
  "name": "ng-env-demo",
  "version": "0.0.0",
  "scripts": {
    "start:app-builder": "lerna exec --scope=@ngx-env/app-builder -- yarn start",
    "start:app-process": "lerna exec --scope=@app-process -- yarn start",
    "start:cli": "lerna run start --scope=@ngx-env/builder",
    "build": "lerna run build",
    "build:cli": "lerna run build --scope=@ngx-env/builder",
    "build:lib": "lerna run build --scope=@ngx-env/core",
    "build:app-builder": "lerna run build --scope=@ngx-env/app-builder",
    "build:app-process": "lerna run build --scope=@ngx-env/app-process",
    "ng-add-builder": "lerna exec --scope=@ngx-env/app-builder -- yarn ng add @ngx-env/builder",
    "ng-add-process": "lerna exec --scope=@ngx-env/app-process -- yarn ng add @ngx-env/builder --environment-only",
    "test": "lerna run test"
  },
  "private": true,
  "devDependencies": {
    "lerna": "^4.0.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {}
}
